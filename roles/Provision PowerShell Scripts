##AH:UseTextMode


set @scripts = @(
    Install-PowerShellModule.ps1,
    Install-PowerShellModule.Tests.ps1
);

# Loop
foreach $scriptName in @scripts
{
    Ensure-Asset
    (
        Name: $scriptName,
        Raft: Default,
        Type: Script,
        Directory: c:\PesterTestsForOtter
    );
}
